		<div class="wrapper">

			<div class="something">
				<p>Heim<br>lich</p>
				<nav class="site-nav">
					<a href="/blog/" title="Thoughts and Random Musings">Blog</a>
					{# }<a href="/projects/" title="Side Projects">Projects</a> #}
					<a href="/speaking/" title="Invited Talks and Speaking Engagements">Speaking</a>
					<a href="/search/">Search</a>
				</nav>
			</div>

			<div class="main-content">
				{{ get_header() }}
				<main>
					{% block content '' %}
				</main>
			</div>
			{{ get_footer() }}
		</div>
		{{ wp_footer() }}

	</body>
</html>
