<!doctype html>
<html class="no-js" {{ get_language_attributes() }}>
	<head>
		<meta charset="UTF-8" />
		<meta http-equiv="Content-Type" content="text/html; charset=UTF-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1.0">
		{{ wp_head() }}
		{% block head '' %}
	</head>
	<body id="" {{ body_class() }}>
		{{ get_header() }}

		<div class="bg-container">
			<div class="overlay-mask"></div>
		</div>
		<div class="main-content" id="main-content">
			<div class="circle-photo-holder">
				<img src="{{ get_template_directory_uri() }}/assets/img/russell-face-square.jpg" alt="Russell Heimlich's face" class="circle-photo" draggable="false">
			</div>
			<main>
				{% block content '' %}
			</main>
		</div>
		{{ get_footer() }}
		{{ wp_footer() }}
	</body>
</html>
